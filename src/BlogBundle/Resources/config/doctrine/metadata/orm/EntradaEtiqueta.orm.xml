<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EntradaEtiqueta" table="entrada_etiqueta">
    <indexes>
      <index name="fk_entrada_has_etiqueta_etiqueta1_idx" columns="etiqueta_id"/>
      <index name="fk_entrada_has_etiqueta_entrada1_idx" columns="entrada_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="etiqueta" target-entity="Etiqueta" fetch="LAZY">
      <join-columns>
        <join-column name="etiqueta_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="entrada" target-entity="Entrada" fetch="LAZY">
      <join-columns>
        <join-column name="entrada_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
